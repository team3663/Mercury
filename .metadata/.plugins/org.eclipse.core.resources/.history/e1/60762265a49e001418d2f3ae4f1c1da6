package org.usfirst.frc.team3663.robot.subsystems;

import edu.wpi.first.wpilibj.command.Subsystem;
import edu.wpi.first.wpilibj.CANTalon;

/**
 *
 */
public class DriveTrainSS extends Subsystem 
{
    // Put methods for controlling this subsystem
    // here. Call these from Commands.
	CANTalon driveTalonL1 = new CANTalon(0);
	CANTalon driveTalonL2 = new CANTalon(1);
	CANTalon driveTalonR1 = new CANTalon(2);
	CANTalon driveTalonR2 = new CANTalon(3);

	double mag = 0.2;
	int encoderValL;
	int encoderValR;
	
    public void initDefaultCommand() 
    {
        // Set the default command for a subsystem here.
        //setDefaultCommand(new MySpecialCommand());
    }
    
    public void encoderDrive()
    {
    	driveTalonL1.set(mag);
    	driveTalonL2.set(mag);
    }
}

